-- binary
04 04 # version, type
00 48 # length
12 34 56 78 # xid
00 5c 16 c7 # experimenter
00 00 00 2e # subtype
00 14 # table_id
00 12 # key_length
fe dc ba 98 76 54 32 10 ff ee cc bb aa 99 88 77 # checksum

00 03 # key[0].class
00 # key[0].type
08 # key[0].length
00 00 00 05 # key[0].value

00 03 # key[1].class
01 # key[1].type
0a # key[1].length
01 23 45 67 89 ab # key[1].value

00 03 # value[0].class
00 # value[0].type
08 # value[0].length
00 00 00 06 # value[0].value

00 03 # value[1].class
01 # value[1].type
0a # value[1].length
ff ee dd cc bb aa # value[1].value
-- python
ofp.message.bsn_table_entry_add(
    xid=0x12345678,
    checksum=0xFEDCBA9876543210FFEECCBBAA998877,
    table_id=20,
    key=[
        ofp.tlv.bsn_test_port(5),
        ofp.tlv.bsn_test_mac([0x01, 0x23, 0x45, 0x67, 0x89, 0xab]),
    ],
    value=[
        ofp.tlv.bsn_test_port(6),
        ofp.tlv.bsn_test_mac([0xff, 0xee, 0xdd, 0xcc, 0xbb, 0xaa]),
    ])
-- java
